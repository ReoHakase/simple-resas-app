{
  "name": "simple-resas-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "prepare": "turbo build --filter='./packages/*'",
    "format:check": "prettier --check --cache .",
    "format": "prettier --write --cache ."
  },
  "devDependencies": {
    "concurrently": "9.1.0",
    "lefthook": "1.8.2",
    "patch-package": "8.0.0",
    "playwright": "1.48.2",
    "prettier": "3.3.3",
    "tsup": "8.3.5",
    "turbo": "2.3.0",
    "typescript": "5.6.3",
    "vitest": "2.1.5"
  },
  "engines": {
    "node": "20",
    "pnpm": "8"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  },
  "packageManager": "pnpm@8.15.9"
}
