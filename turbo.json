{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {
      "dependsOn": ["build"],
      "outputs": [],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": ["NODE_ENV"]
    },
    "build-fast": {
      "dependsOn": ["^build-fast"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test": {
      "outputs": []
    },
    "sb:build": {
      "dependsOn": ["^build"],
      "outputs": ["storybook-static/**"]
    },
    "sb:test": {
      "dependsOn": ["sb:build"]
    },
    "playwright:test": {
      "dependsOn": ["build"],
      "outputs": ["playwright-report/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
